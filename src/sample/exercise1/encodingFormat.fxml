<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="224.0" prefWidth="555.0" xmlns="http://javafx.com/javafx/11.0.2"
            fx:controller="sample.exercise1.EncodingFormatController">
    <top>
        <HBox alignment="CENTER" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Specify a Unicode">
                <font>
                    <Font size="18.0"/>
                </font>
            </Text>
            <TextField fx:id="startSymbol" onAction="#show"/>
        </HBox>
    </top>
    <center>
        <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <TextArea fx:id="textArea" editable="false">
                <font>
                    <Font size="18.0"/>
                </font>
            </TextArea>
        </ScrollPane>
    </center>
</BorderPane>
